										"exec": [
											"var jsonData = JSON.parse(responseBody);",
											"var req = JSON.parse(request.data);",
											"if (req.type === \"PRIVATE\"){",
											"    postman.setEnvironmentVariable(\"private_id_bundle\", jsonData.idBundle);",
											"}",
											"else if (req.type === \"PUBLIC\"){",
